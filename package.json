{
  "name": "spa-console-billing",
  "version": "0.0.0",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack --mode=production",
    "analyze": "webpack --mode=production --env.analyze=true",
    "test": "npm run test:unit && npm run test:coverage",
    "tsc:interface": "kushki json2ts",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "lint": "npm run lint:format && npm run lint:duplicate && npm run lint:check && eslint --fix \"./src/**\"",
    "lint:check": "tsc --noEmit && eslint --ext=jsx,ts,tsx src",
    "lint:duplicate": "jscpd --ignore \"src/schema/*.json\" ./src",
    "lint:fix": "npm run lint:check --fix",
    "lint:format": "prettier --write \"src/**/*.ts?(x)\" \"*.json\"",
    "depcheck": "depcheck . --ignores=\"@babel/*,@types/*,@kushki/*,@testing-library/*,@mui/icons-material,css-loader,identity-*,redux,pretty-quick,tslib,style-loader,concurrently,ss-loader,eslint-*,husky,webpack-*,babel-loader,react,react-dom,prettier,eslint-config-react-app,babel-eslint,eslint-plugin-import,eslint-plugin-flowtype,eslint-plugin-jsx-a11y,lodash,history,date-fns,file-loader,systemjs-webpack-interop,axios,webpack,@aws-amplify/auth,@material-ui/icons,@material-ui/lab,react-hook-form,react-router,redux-mock-store,jest-transform-stub,@mui,@typescript-eslint/parser,@typescript-eslint/parser,@typescript-eslint/eslint-plugin,@mui/lab,@mui/styles,react-feather,@mui/system\"",
    "validate": "npm run depcheck && npm run lint && npm run test"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime": "^7.8.7",
    "@kushki/cli": "1.4.1",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^25.2.1",
    "@types/react": "^17.0.26",
    "@types/react-dom": "16.9.6",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "^5.1.4",
    "@types/redux-mock-store": "^1.0.2",
    "@types/single-spa-react": "2.12.0",
    "@types/systemjs": "^6.1.0",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-jest": "^24.9.0",
    "concurrently": "^5.0.1",
    "depcheck": "1.4.2",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-config-ts-react-important-stuff": "2.0.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "file-loader": "^6.0.0",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.7",
    "jest-cli": "^25.2.7",
    "jscpd": "^3.4.2",
    "jwt-decode": "^3.1.2",
    "prettier": "2.5.1",
    "pretty-quick": "^2.0.1",
    "single-spa-react": "^2.14.0",
    "systemjs-webpack-interop": "^2.3.4",
    "tslib": "^2.3.1",
    "typescript": "4.5.5",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-config-single-spa-react": "^1.0.3",
    "webpack-config-single-spa-react-ts": "^1.4.2",
    "webpack-config-single-spa-ts": "^1.4.2",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@emotion/react": "11.7.0",
    "@emotion/styled": "11.6.0",
    "@kushki/connect-ui": "0.0.106",
    "@kushki/security-wrapper": "5.1.5",
    "@mui/icons-material": "^5.4.1",
    "@mui/lab": "5.0.0-alpha.68",
    "@mui/material": "5.2.3",
    "@mui/styles": "5.2.3",
    "@reduxjs/toolkit": "1.7.2",
    "axios": "^0.21.1",
    "date-fns": "2.21.3",
    "firebase": "7.14.3",
    "lodash": "^4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-feather": "^2.0.9",
    "react-redux": "^7.2.2",
    "react-router": "5.1.2",
    "react-router-dom": "6.3.0",
    "redux": "4.1.2",
    "redux-mock-store": "^1.5.4"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.1.1",
    "**/@typescript-eslint/parser": "^4.1.1"
  }
}
